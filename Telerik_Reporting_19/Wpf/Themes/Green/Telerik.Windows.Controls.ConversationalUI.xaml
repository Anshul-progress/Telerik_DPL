                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="CreationDateTextBlock" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="AuthorNameTextBlock" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
