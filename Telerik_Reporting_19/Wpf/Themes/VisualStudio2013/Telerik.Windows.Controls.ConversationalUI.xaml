                                </Grid>
                            </telerik1:RadToggleButton>
                            <Grid Grid.Row="1" Grid.Column="1" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Path=IsButtonPartVisible, Converter={StaticResource BooleanToVisibilityConverter}}">
                                <Border x:Name="MouseOverButtonVisual"
