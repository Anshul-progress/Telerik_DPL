                            <Setter Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=AccentBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=AccentBrush}"/>
