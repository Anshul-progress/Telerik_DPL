                                        MinHeight="22"
                                        IsBackgroundVisible="False">
                                    <telerik:RadGlyph Glyph="{StaticResource GlyphArrow60Right}"/>
                                </telerik:RadButton>
                                <telerik:RadButton x:Name="PART_MoveToLastPage"
