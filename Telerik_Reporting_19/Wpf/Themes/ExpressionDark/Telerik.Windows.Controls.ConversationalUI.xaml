                                        Margin="4 0"
                                        Padding="0"
                                        MinWidth="22"
                                        MinHeight="22"
                                        IsBackgroundVisible="False">
                                    <telerik:RadGlyph Glyph="{StaticResource GlyphArrow60Left}"/>
                                </telerik:RadButton>
                                <telerik:RadButton x:Name="PART_MoveToNextPage"
