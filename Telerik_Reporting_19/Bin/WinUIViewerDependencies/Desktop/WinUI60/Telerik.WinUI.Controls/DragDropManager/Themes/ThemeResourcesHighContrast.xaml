<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:telerik="using:Telerik.UI.Xaml.Controls">

	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="ms-appx:///Telerik.WinUI.Controls/Primitives/Themes/ThemeColorsHighContrast.xaml"/>
	</ResourceDictionary.MergedDictionaries>

	<!-- DragVisual start -->
    <SolidColorBrush x:Key="TelerikDragDrop_DragVisualForeground" Color="{ThemeResource SystemColorWindowTextColor}" />
    <SolidColorBrush x:Key="TelerikDragDrop_DragVisualBackground" Color="{ThemeResource SystemColorWindowColor}" />
    <SolidColorBrush x:Key="TelerikDragDrop_DragVisualBorderBrush" Color="{ThemeResource SystemColorWindowTextColor}" />
    <SolidColorBrush x:Key="TelerikDragDrop_DragVisualMultiItemBackground" Color="{ThemeResource SystemColorWindowColor}" />
    <SolidColorBrush x:Key="TelerikDragDrop_DragVisualMultiItemBorderBrush" Color="{ThemeResource SystemColorWindowTextColor}" />
    <SolidColorBrush x:Key="TelerikDragDrop_DragVisualNormalBrush" Color="{ThemeResource SystemColorWindowTextColor}" />
    <SolidColorBrush x:Key="TelerikDragDrop_DragVisualImportantBrush" Color="#A80000" />
    <!-- DragVisual end -->
</ResourceDictionary>

